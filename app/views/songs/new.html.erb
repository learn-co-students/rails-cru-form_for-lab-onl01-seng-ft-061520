
<h1>New Song Form</h1>

<%= form_for(@song) do |new_song_form| %>
    <label>Song name</label></br>
    <%= new_song_form.text_field :name %>

    <label>Song artist</label></br>
    <%= new_song_form.text_field :artist_id %>

    <label>Song genre</label></br>
    <%= new_song_form.text_field :genre_id %>

    <%= new_song_form.submit %>
<% end %>
